$(function() {
 	switch (activeMenu) {
 case 'home':
		$('#home').addClass('active')
 		break;
 	case 'index':
 		$('#home').addClass('active')
 		break;
 //		pages mapping
 	case 'login':
 		$('#pages').addClass('active')
 		break;
 	case 'login':
 		$('#pages').addClass('active')
 		break;
 	case 'login':
 		$('#pages').addClass('active')
 		break;
 		
 	case 'about':
 				$('#pages').addClass('active')
 				break;
 			case 'service':
 		 		$('#pages').addClass('active')
 		 		break;
 		 	case 'error':
 		 		$('#pages').addClass('active')
 		 		break;
 		 	case 'aboutMe':
 		 		$('#pages').addClass('active')
 		 		break;
 		 	case 'FAQ':
 		 		$('#pages').addClass('active')
 		 		break;
 		 		
 		 	case 'category':
 		 		$('#category').addClass('active')
 		 		break;
 		 	case'contact':
 		 		$('#contact').addClass('active')
 		 		break;
 		 		
 		 		
 		 	case 'adminAddProduct':
 		 		$('#admin').addClass('active')
 		 		break;
 		 		
 			case 'addProduct':
 		 		$('#admin').addClass('active')
 		 		break;
 		 		
 			case 'editProduct':
 		 		$('#admin').addClass('active')
 		 		break;
 		 		
 			case 'addCategory':
 		 		$('#admin').addClass('active')
 		 		break;
 			case 'AdminAddCategory':
 		 		$('#admin').addClass('active')
 		 		break;
 			case 'editCategory':
 		 		$('#admin').addClass('active')
 		 		break;
 			case 'adminAddSupplier':
 		 		$('#admin').addClass('active')
 		 		break;
 			case 'addSupplier':
 		 		$('#admin').addClass('active')
 		 		break;
 			case 'editSupplier':
 		 		$('#admin').addClass('active')
 		 		break;
 		 	
 		 		
 		   case 'product':
 				$('#product').addClass('active')
 				 		break;
 				 	case 'productItem':
 						$('#product').addClass('active')
 				 		break;
 				 	 case 'productShow':
 		 				$('#product').addClass('active')
 		 				 		break;
 		 		default:
 			$("#home").addClass('active')
 			break;
 	}
});